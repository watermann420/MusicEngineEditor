  <ItemGroup>
    <ProjectReference Include="..\MusicEngine\MusicEngine.csproj" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Editor\CSharpScript.xshd" />
  </ItemGroup>

</Project>
