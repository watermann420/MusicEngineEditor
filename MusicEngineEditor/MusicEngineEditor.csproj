    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>MusicEngineEditor</AssemblyName>
    <RootNamespace>MusicEngineEditor</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
